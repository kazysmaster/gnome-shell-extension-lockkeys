#~ msgstr "Только Num-Lock"

#~ msgid "Caps-Lock Only"
#~ msgstr "Только Caps-Lock"

#~ msgid "Both"
#~ msgstr "Оба"

#~ msgid "Show/Hide"
#~ msgstr "Показать/Скрыть"

#~ msgid "Show/Hide Caps-Lock Only"
#~ msgstr "Показать/Скрыть только Caps-Lock"
